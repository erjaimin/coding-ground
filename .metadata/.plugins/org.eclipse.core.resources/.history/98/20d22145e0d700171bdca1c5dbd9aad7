package com.main;

import javax.swing.JPanel;

import com.mvc.view.BottomPerspectiveView;
import com.mvc.view.ImageView;
import com.mvc.view.TopPerspectiveView;

public class PatternObjectFactroy {

	public static JPanel getJpanel(String viewName){
		if(viewName.equals(Constants.IMAGE_VIEW)){
			return new ImageView();
		}else if(viewName.equals(Constants.TOP_VIEW)){
			return new TopPerspectiveView();
		}else if(viewName.equals(Constants.BOTTOM_VIEW)){
			return new BottomPerspectiveView();
		}
		return null;
	}
}
