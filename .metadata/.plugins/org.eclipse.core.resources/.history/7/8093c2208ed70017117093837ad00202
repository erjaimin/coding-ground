package com.main;

import javax.swing.SwingUtilities;

public class Client {
	public static void main(String [] args) {
        SwingUtilities.invokeLater(new Runnable() {
            @Override
            public void run() {
                new Client().start();
            }
        });
    }
    private void start() {
        MainWindow m = new MainWindow();
        new DialogWindow(m);
    }
}
